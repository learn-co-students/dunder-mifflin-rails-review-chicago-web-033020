<h1>Employee Edit Form</h1>

<%= form_for(@employee) do |f| %>
    <%= f.label :first_name, "First name:" %>
    <%= f.text_field :first_name %>
    <br>
    <br>
    <%= f.label :last_name, "Last name:" %>
    <%= f.text_field :last_name %>
    <br>
    <br>
    <%= f.label :alias, "Alias:" %>
    <%= f.text_field :alias %>
    <br>
    <br>
    <%= f.label :title, "Title:" %>
    <%= f.text_field :title %>
    <br>
    <br>
    <%= f.label :office, "Office:" %>
    <%= f.text_field :office %>
    <br>
    <br>
    <%= f.label :img_url, "Image URL:" %>
    <%= f.text_field :img_url %>
    <br>
    <br>
    <!-- Dog -->
    <%= f.label :dog_name, "Dog:" %>
    <%= f.collection_select :dog_name, Dog.all, :name, :name %>
    <br>
    <br>
    <%= f.submit %>
<% end %>